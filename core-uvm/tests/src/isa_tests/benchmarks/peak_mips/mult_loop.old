  blez a0, fin_loop
ini_loop:
  mul x6, x6, x6
  mul x7, x7, x7
  mul x28, x28, x28
  mul x29, x29, x29
  mul x30, x30, x30
  mul x31, x31, x31
  mul x6, x6, x6
  mul x7, x7, x7
